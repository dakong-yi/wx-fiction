.name_list {
    width: 100%;
    text-align: center;
    font-size: 12px;
    color: #3876b2;
    margin-top: 15px;
}
.name_list navigator {
    float: left;
    width: 33.333%;
    margin-bottom: 15px;
}
.name_list navigator .inner-item {
    width: 85%;
    margin: 0 auto;
}
.name_list navigator .inner-item image {
    width: 100%;
    height: 140px;
}
.name_list navigator.add-book {}
.name_list navigator.add-book .inner-item {
    height:140px;
    border:1px solid #ddd;
    background:#f4f4f4;
    font-size:45px;
    line-height:134px;
    color:#ccc;
}
.no-book {
    position:absolute;
    font-size:12px;
    color:#333;
    text-align:center;
    top:50%;
    margin-top:-16px;
}
.no-book text {}
.no-book navigator {
    color: #3876b2;
}